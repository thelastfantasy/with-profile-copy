    "tampermonkey",
    "userscript",
    "with.is",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/tampermonkey": "^4.0.5",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  }
}